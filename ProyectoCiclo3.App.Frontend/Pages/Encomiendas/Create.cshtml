@page
@model ProyectoCiclo3.App.Frontend.Pages.FormEncomiendaModel
@{
}
<div class="text-center">
      <form method="post">
            <label for="descripcion" class="form-label">Descripcion:</label>
            <input type="text" class="form-control" id="descripcion" name="descripcion" placeholder="Descripcion"
                  required>
            <label for="peso" class="form-label">Peso producto:</label>
            <input type="float" class="form-control" id="peso" name="peso" placeholder="Peso" required>
            <label for="tipo" class="form-label">Tipo:</label>

            <input type="text" class="form-control" id="tipo" name="tipo" placeholder="Tipo" required>
            <label for="presentacion" class="form-label">Presentacion:</label>
            <input type="text" class="form-control" id="presentacion" name="presentacion" placeholder="Presentacion"
                  required>

            <hr>
            <button type="submit" class="btn btn-success">Enviar</button>
      </form>
</div>
